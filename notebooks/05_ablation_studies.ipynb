# 05_ablation_studies.ipynb
import torch
from torch.utils.data import DataLoader
import matplotlib.pyplot as plt
from models.transformer import PVTransformer
from models.vit_encoder import ViTEncoder
from models.gnn_model import GNNModel
from training.train_multimodal import train_model
import pandas as pd

# Load processed data
train = pd.read_csv('../data/processed/train.csv')
test = pd.read_csv('../data/processed/test.csv')

# Ablation experiments
experiments = {
    'Transformer_only': PVTransformer(input_dim=6, d_model=64, nhead=4, num_layers=2),
    'ViT+Transformer': ViTEncoder(embed_dim=64),
    'GNN+Transformer': GNNModel(node_features=6, hidden_dim=32)
}

results = {}
for name, model in experiments.items():
    print(f'Running experiment: {name}')
    rmse, mae = train_model(model, train, test, epochs=5)
    results[name] = {'RMSE': rmse, 'MAE': mae}

# Compare results
plt.figure(figsize=(8,5))
names = list(results.keys())
rmse_vals = [results[n]['RMSE'] for n in names]
plt.bar(names, rmse_vals, color='blue')
plt.title('Ablation Study: RMSE Comparison')
plt.ylabel('RMSE')
plt.show()

